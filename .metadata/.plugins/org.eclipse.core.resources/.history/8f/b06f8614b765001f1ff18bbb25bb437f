package edu.kh.op.ex;

public class ExampleRun { //코드 실행용 클래스로 만든것

	//메인메서드 필수 작성
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// OpExample 생성
		// -> OpExample이라는 클래스(설계도)를 이용해서 객체를 생성하는데, 그 객체 이름이 ex다.
		OpExample opEx = new OpExample(); // opexample을 담을 상자를 만드는데, 담아야하니까 형식도 opexample 이여야한다
		// 같은 패키지(edu.kh.op.ex)안에 있는 클래스는 import를 하지 않아도 불러다 쓸 수 있다!
		opEx.ex1(); // opEx가 가지고있는 ex1() 메서드 시랳ㅇ
		
		
		
		
		
		
		

	}

}
